nir --dataset_dir ./datasets/suramin/ --output_dir new_train_dir/NIR_GRU_suramin --num_example 50 --epochs 400 --model GRU --batch_size 256 --num_workers 0

# Animals
nir --dataset_dir ./datasets/animals/ --output_dir new_train_dir/NIR_Transformer_animals --num_example 20 --epochs 400 --model Transformer --batch_size 256 --num_workers 0
