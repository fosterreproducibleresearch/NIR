# Nctrer
nir --dataset_dir ./datasets/nctrer/ --output_dir new_train_dir/NIR_Transformer_nctrer --num_example 50 --epochs 400 --model Transformer --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/nctrer/ --output_dir new_train_dir/NIR_LSTM_nctrer --num_example 50 --epochs 400 --model LSTM --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/nctrer/ --output_dir new_train_dir/NIR_GRU_nctrer --num_example 50 --epochs 400 --model GRU --batch_size 256 --num_workers 0

# Mutagenesis
nir --dataset_dir ./datasets/mutagenesis/ --output_dir new_train_dir/NIR_Transformer_mutagen --num_example 100 --epochs 400 --model Transformer --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/mutagenesis/ --output_dir new_train_dir/NIR_LSTM_mutagen --num_example 100 --epochs 400 --model LSTM --batch_size 256 --num_workers 0
