#!/bin/bash
# Family
nir --dataset_dir ./datasets/family/ --output_dir new_train_dir/NIR_Transformer_family --num_example 50 --epochs 400 --model Transformer --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/family/ --output_dir new_train_dir/NIR_LSTM_family --num_example 50 --epochs 400 --model LSTM --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/family/ --output_dir new_train_dir/NIR_GRU_family --num_example 50 --epochs 400 --model GRU --batch_size 256 --num_workers 0

# Semantic Bible
nir --dataset_dir ./datasets/semantic_bible/ --output_dir new_train_dir/NIR_Transformer_semb --num_example 50 --epochs 400 --model Transformer --batch_size 256 --num_workers 0
nir --dataset_dir ./datasets/semantic_bible/ --output_dir new_train_dir/NIR_LSTM_semb --num_example 50 --epochs 400 --model LSTM --batch_size 256 --num_workers 0
