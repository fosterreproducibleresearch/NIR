#!/bin/bash
# Nctrer
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/nctrer/ --model Transformer --output_dir Results/NIR_Transformer_Eval_nctrer --pretrained_model_path new_train_dir/NIR_Transformer_nctrer/ --caption "Retrieval results on Nctrer using the Transformer architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/nctrer/ --model LSTM --output_dir Results/NIR_LSTM_Eval_nctrer --pretrained_model_path new_train_dir/NIR_LSTM_nctrer --caption "Retrieval results on Nctrer using the LSTM architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/nctrer/ --model GRU --output_dir Results/NIR_GRU_Eval_nctrer --pretrained_model_path new_train_dir/NIR_GRU_nctrer/ --caption "Retrieval results on Nctrer using the GRU architecture"

## Family
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/family/ --model Transformer --output_dir Results/NIR_Transformer_Eval_family --pretrained_model_path new_train_dir/NIR_Transformer_family/ --caption "Retrieval results on Family using the Transformer architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/family/ --model LSTM --output_dir Results/NIR_LSTM_Eval_family --pretrained_model_path new_train_dir/NIR_LSTM_family/ --caption "Retrieval results on Family using the LSTM architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/family/ --model GRU --output_dir Results/NIR_GRU_Eval_family --pretrained_model_path new_train_dir/NIR_GRU_family/ --caption "Retrieval results on Family using the GRU architecture"
## Semantic Bible
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/semantic_bible/ --model Transformer --output_dir Results/NIR_Transformer_Eval_semb --pretrained_model_path new_train_dir/NIR_Transformer_semb/ --caption "Retrieval results on Semantic Bible using the Transformer architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/semantic_bible/ --model LSTM --output_dir Results/NIR_LSTM_Eval_semb --pretrained_model_path new_train_dir/NIR_LSTM_semb/ --caption "Retrieval results on Semantic Bible using the LSTM architecture"
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/semantic_bible/ --model GRU --output_dir Results/NIR_GRU_Eval_semb --pretrained_model_path new_train_dir/NIR_GRU_semb/ --caption "Retrieval results on Semantic Bible using the GRU architecture"
