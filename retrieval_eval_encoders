python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/lymphography/ --model GRU --output_dir Results/NIR_GRU_Eval_lymph --pretrained_model_path new_train_dir/NIR_GRU_lymph/ --caption "Retrieval results on Lymphography using the GRU architecture"
## Suramin
python nir/scripts/retrieval_eval.py --dataset_dir ./datasets/suramin/ --model Transformer --output_dir Results/NIR_Transformer_Eval_suramin --pretrained_model_path new_train_dir/NIR_Transformer_suramin/ --caption "Retrieval results on Suramin using the Transformer architecture"
